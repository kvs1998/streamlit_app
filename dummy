from pathlib import Path

# Current file: dashboard/app_driver.py
# Going to: dashboard/images/blackrock_logo.png

# Path relative to app_driver.py
image_path = Path(__file__).parent / "images" / "blackrock_logo.png"

# Example: reading as binary
with open(image_path, "rb") as f:
    data = f.read()

print(f"Read image successfully, {len(data)} bytes.")
